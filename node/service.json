{
  "memory": 512,
  "ttl": 60,
  "timeout": 30,
  "minReplicas": 2,
  "maxReplicas": 10,
  "workers": 1,
  "routes": {
    "getFallbackTaxes": {
      "path": "/_v/tax-fallback/:country/:provider/:postalCode",
      "public": true
    },
    "downloadFallbackTaxes": {
      "path": "/_v/tax-fallback/download-taxes/:country/:provider",
      "public": true
    }
  },
  "events": {
    "taxUpdate": {
      "sender": "vtex.tax-fallback",
      "keys": ["taxFallback.taxUpdate"]
    }
  }
}
